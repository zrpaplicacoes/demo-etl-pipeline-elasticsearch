{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "meta": {
      "title": "zrp meet#2: ETL demo",
      "description": "This application is a demo app that uses node streams to extract, transform and push data from two sources, openflights and a gzip csv file containing recipes from kaggle on ElasticSearch for querying and analysis, implementing a simple ETL pipeline."
    },
    "search": true,
    "collapse": false,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "GitHub": {
        "href": "https://github.com/zrpaplicacoes/demo-etl-pipeline-elasticsearch",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  }
}
